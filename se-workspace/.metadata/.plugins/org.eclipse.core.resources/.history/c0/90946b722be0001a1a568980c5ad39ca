 package Operations;

public class IncreaseDecreaseOperationExample {
	public static void main(String[] args) {
		int x=10;
		int y=10;
		int z;
		
		System.out.println("*************************");
		x++;
		++x;
		System.out.println("x="+x); //12
		
		y--;
//		--y;
		System.out.println("y="+y); //8
	}
}

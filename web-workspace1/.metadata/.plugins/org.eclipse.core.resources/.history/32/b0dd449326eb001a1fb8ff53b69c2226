CREATE TABLE menuforStudy
	(id varchar2(100) primary key,
	menu varchar2(100) not null,
	price number not null
	)
	
SELECT*FROM menuforStudy
CREATE sequence menuforStudy_seq;

INSERT INTO menuforStudy(id,menu,price)VALUES(menuforStudy_seq.nextval,'제육볶음',5000);

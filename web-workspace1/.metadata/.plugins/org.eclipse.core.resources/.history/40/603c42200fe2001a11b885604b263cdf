<script type="text/javascript">
	function testArray1(){
		var arr=new Array(3);
		arr[0]="김수민";
		arr[1]="진용현";
		arr[2]="양성식";
		for(var i=0;i<arr.length;i++){
			alert(arr[i]);
		}
	}
</script>
